<Window x:Class="Fanytel.TransfersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fanytel"
        mc:Ignorable="d"
        Title="TransfersWindowxaml" Height="300" Width="398.293"
        Loaded="Window_Loaded">
    <Grid>
        <DatePicker HorizontalAlignment="Left" Margin="23,10,0,0" VerticalAlignment="Top" 
                    Width="164" Name="datePickerFrom"/>
        <DatePicker HorizontalAlignment="Right" Margin="0,10,10,0" VerticalAlignment="Top" 
                    Width="164" Name="datePickerTo"/>
        <TextBlock HorizontalAlignment="Left" Margin="28,105,0,0" TextWrapping="Wrap" 
                   Text="Credits" VerticalAlignment="Top" FontSize="40"/>
        <TextBlock HorizontalAlignment="Left" Margin="28,181,0,0" TextWrapping="Wrap" 
                   Text="Money" VerticalAlignment="Top" FontSize="40"/>
        <TextBox x:Name="textBoxCredits" HorizontalAlignment="Left" Margin="194,104,0,0"
                 TextWrapping="Wrap" Text="0" VerticalAlignment="Top" FontSize="40"/>
        <TextBox x:Name="textBoxMoney" HorizontalAlignment="Left" Margin="194,191,0,0" 
                 TextWrapping="Wrap" Text="0" VerticalAlignment="Top" FontSize="40"/>
    </Grid>
</Window>
